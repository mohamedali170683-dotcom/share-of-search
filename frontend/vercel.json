{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "(prisma generate || true) && npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "api/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/projects/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
