# Database URL for Prisma (PostgreSQL)
# For Vercel Postgres, Neon, Supabase, or other PostgreSQL providers
DATABASE_URL="postgresql://user:password@host:5432/database?sslmode=require"

# Direct URL for Prisma (without connection pooling)
DIRECT_URL="postgresql://user:password@host:5432/database?sslmode=require"

# Example using Vercel Postgres:
# DATABASE_URL="${POSTGRES_PRISMA_URL}"
# DIRECT_URL="${POSTGRES_URL_NON_POOLING}"

# Example using Neon:
# DATABASE_URL="postgresql://user:pass@ep-xyz.us-east-2.aws.neon.tech/neondb?sslmode=require"
# DIRECT_URL="postgresql://user:pass@ep-xyz.us-east-2.aws.neon.tech/neondb?sslmode=require"
